{
  "name": "obsidian-firesync",
  "version": "0.1.0",
  "description": "Sync your Obsidian vault with Firebase",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "test": "vitest ./src/",
    "test:ci": "vitest run ./src/",
    "cors": "gcloud storage buckets update gs://${BUCKET} --cors-file=cors.json",
    "cors:show": "gcloud storage buckets describe gs://${BUCKET} --format='default(cors)'",
    "deploy:rules": "npx firebase deploy --only firestore:rules,storage"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@tsconfig/svelte": "^4.0.1",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "5.58.0",
    "@typescript-eslint/parser": "5.58.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.16",
    "esbuild-svelte": "^0.7.3",
    "firebase-tools": "^11.28.0",
    "jsdom": "^21.1.1",
    "obsidian": "latest",
    "svelte": "^3.58.0",
    "svelte-preprocess": "^5.0.3",
    "ts-jest": "^29.1.0",
    "tslib": "2.5.0",
    "typescript": "5.0.4",
    "vitest": "^0.30.1"
  },
  "dependencies": {
    "firebase": "^9.19.1"
  }
}
